{
  "name": "istanbul-diff",
  "version": "1.0.5",
  "description": "Diffs istanbul json summary coverage reports",
  "main": "lib/index.js",
  "bin": "bin/cli.js",
  "dependencies": {
    "chalk": "^1.1.3",
    "jsondiffpatch": "^0.2.4",
    "lodash-deep": "^2.0.0",
    "minimist": "^1.2.0",
    "nicejob": "^1.0.0",
    "omit-deep-lodash": "^0.8.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.1.0"
  },
  "scripts": {
    "test": "mocha",
    "test-cover": "istanbul cover --report html --report json-summary node_modules/.bin/_mocha",
    "sample-inc": "./bin/cli.js test/data/coverage2.json test/data/coverage1.json",
    "sample-dec": "./bin/cli.js test/data/coverage1.json test/data/coverage2.json --nofail",
    "sample-same": "./bin/cli.js test/data/coverage1.json test/data/coverage1.json"
  },
  "keywords": [
    "istanbul",
    "coverage"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moos/istanbul-diff.git"
  },
  "author": "moos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moos/istanbul-diff/issues"
  },
  "homepage": "https://github.com/moos/istanbul-diff#readme"
}
